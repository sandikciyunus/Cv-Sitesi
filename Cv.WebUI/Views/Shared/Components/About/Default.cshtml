@model Cv.WebUI.Models.AppUserListViewModel2

@foreach (var item in Model.AppUsers)
{
    <div class="resume-section-content">
        <h1 class="mb-0">
           @item.FirstName
            <span class="text-primary">@item.LastName</span>
        </h1>
        <div class="subheading mb-5">
            @item.Address,· @item.PhoneNumber ·
            <a href="mailto:name@email.com">@item.Email</a>
        </div>
        <p class="lead mb-5">@item.ShortDescription</p>
        <div class="social-icons">
            <a class="social-icon" target="_blank" href="@item.LınkedinLink"><i class="fab fa-linkedin-in"></i></a>
            <a class="social-icon" target="_blank" href="@item.GithubLink"><i class="fab fa-github"></i></a>
        </div>
    </div>
}
